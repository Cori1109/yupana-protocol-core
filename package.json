{
  "name": "Yupana",
  "version": "1.0.0",
  "description": "Yupana protocol",
  "dependencies": {
    "@taquito/michelson-encoder": "^9.0.1",
    "@taquito/rpc": "^9.0.1",
    "@taquito/signer": "^9.0.1",
    "@taquito/taquito": "^9.0.1",
    "child_process": "^1.0.2",
    "dotenv": "^10.0.0",
    "fs": "^0.0.1-security",
    "ganache-cli": "6.12.1-tezos.0",
    "mocha": "^8.4.0",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "truffle": "5.5.0-tezos.4",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2",
    "yargs": "^17.0.1",
    "commander": "^7.0.0"
  },
  "scripts": {
    "postinstall": "patch-package",
    "clean": "rm -rf ./build",
    "compile": "yarn clean && node scripts/cli.js compile -c Controller && node scripts/cli.js compile -c Factory && node scripts/cli.js compile -c getOracle && node scripts/cli.js compile -c qToken && node scripts/cli.js compile -c XTZ",
    "compile-qToken": "node scripts/helpers.js build-qToken --no-json -o contracts",
    "migrate": "yarn compile && node scripts/cli.js migrate",
    "migrate-s": "yarn compile && node scripts/cli.js migrate --network development_server",
    "test": "yarn compile && mocha --bail",
    "start-sandbox": "docker run --rm --name my-sandbox -e flextesa_node_cors_origin='*' --detach -p 8732:20000 tqtezos/flextesa:20210602 flobox start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madfish-solutions/yupana-protocol-core"
  },
  "author": "MadFish.Solutions",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/madfish-solutions/yupana-protocol-core/issues"
  },
  "homepage": "https://github.com/madfish-solutions/yupana-protocol-core#readme",
  "keywords": [
    "tezos",
    "ligo",
    "blockchain",
    "truffle"
  ]
}
