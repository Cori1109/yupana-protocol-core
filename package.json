{
  "name": "tezos-example",
  "version": "2.0.0",
  "description": "Tezos Example Box =================",
  "dependencies": {
    "@taquito/michelson-encoder": "latest",
    "@taquito/signer": "^8.0.4-beta.0",
    "@taquito/taquito": "^8.0.4-beta-RC.0",
    "commander": "^7.0.0",
    "dotenv": "^8.2.0",
    "ganache-cli": "6.12.1-tezos.0",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "truffle": "^5.5.0-tezos.4",
    "truffle-assertions": "^0.9.2"
  },
  "devDependencies": {
    "mocha": "^8.1.1"
  },
  "scripts": {
    "clean": "rm -rf ./build",
    "start-sandbox": "docker run --rm --name my-sandbox -e flextesa_node_cors_origin=\"*\" --detach -p 8732:20000 tqtezos/flextesa:20210316 flobox start",
    "compile-qToken": "node scripts/cli.js build-qToken --no-json -o contracts",
    "migrate": "yarn clean && yarn truffle migrate",
    "migrate-edonet": "yarn clean && yarn truffle migrate --network edonet",
    "compile": "yarn clean && yarn truffle compile",
    "test": "yarn truffle test",
    "postinstall": "patch-package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/truffle-box/tezos-example-box.git"
  },
  "keywords": [
    "tezos",
    "ligo",
    "blockchain",
    "truffle"
  ],
  "author": "Cruz Molina <cruz@trufflesuite.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/truffle-box/tezos-example-box/issues"
  },
  "homepage": "https://github.com/truffle-box/tezos-example-box#readme"
}
